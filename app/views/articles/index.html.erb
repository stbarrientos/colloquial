<div class="well">
  <%= form_tag root_path, method: :get, class: "navbar-form navbar-left search-form" do %>
    <%= text_field_tag(
        :search,
        params[:search],
        placeholder: "Search",
        class: "form-control"
    ) %>
  <% end %>
</div>

<div class="well">
  <ul>
    <% @articles.each do |a| %>
    <li><%= a.title %></li>
    <% end %>
  </ul>
</div>
